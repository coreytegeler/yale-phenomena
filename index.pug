doctype html
- var ver = Math.round((new Date()).getTime() / 1000);
html(lang='en-us', style='height: 100%; visibility: visible;')
	head
		title Mapping the Yale Grammatical Diversity Project
		meta(charset='utf-8')
		meta(name='viewport' content='initial-scale=1,maximum-scale=1,user-scalable=no')
		script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js')
		script(src='https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js')
		script(src='https://cdnjs.cloudflare.com/ajax/libs/clipboard.js/2.0.0/clipboard.min.js')
		link(href='https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap-grid.min.css' rel='stylesheet')
		script(src='https://api.tiles.mapbox.com/mapbox-gl-js/v0.39.1/mapbox-gl.js')
		script(src='./assets/scripts.js?v='+ver)
		link(href='./assets/style.css?v='+ver rel='stylesheet')
	body
		include partials/creation.pug
		#frame(data-mapbox='MAPBOX_VARS')
			include partials/phenomena.pug
			include partials/filters.pug
			footer
				a(href='https://ygdp.yale.edu/').logo
					object(type='image/svg+xml' data='./assets/img/ygdp.svg') Yale Grammatical Diversity Project
				.keys
					.key.reject
						span Unacceptable
					.key.accept
						span Acceptable
			main
				#map